<% @godparents[:ana_claudia_hist] = $ana_claudia_history if $ana_claudia_history %>
<% @godparents[:carlos_hist] = $carlos_history if $carlos_history %>
<% @godparents[:cleiton_hist] = $cleiton_history if $cleiton_history %>
<% @godparents[:diogo_hist] = $diogo_history if $diogo_history %>
<% @godparents[:isabel_hist] = $isabel_history if $isabel_history %>
<% @godparents[:isa_edi_hist] = $isa_edi_history if $isa_edi_history %>
<% @godparents[:isis_hist] = $isis_history if $isis_history %>
<% @godparents[:jana_al_hist] = $jana_al_history if $jana_al_history %>
<% @godparents[:maiara_hist] = $maiara_history if $maiara_history %>
<% @godparents[:maria_hist] = $maria_history if $maria_history %>
