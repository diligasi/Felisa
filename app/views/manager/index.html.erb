<% @users_list = $users_list if $users_list %>
<div class="container" style="width: 80%; height: 75%;  background: rgba(68, 68, 68, .4);">
  <div class="tabContainer">
    <!-- Labels -->
    <div id="managerTabOne" class="tabLabel selected">História do casal</div>
    <div id="managerTabTwo" class="tabLabel">Gerenciar convidados</div>
    <div id="managerTabThree" class="tabLabel">Gerenciar texto dos pais, padrinhos e das crianças</div>
    <div id="managerTabFour" class="tabLabel">Gerenciamento de usuários administradores</div>

    <!-- Labels containers -->
    <div class="tabLabelContainer managerTabOne clearfix selected">
      <%= render partial: 'manager/couple_history_manager', locals: { :content => @couple_history } %>
    </div>
    <div class="tabLabelContainer managerTabTwo clearfix">
      <%= render partial: 'manager/guests_mnager' %>
    </div>
    <div class="tabLabelContainer managerTabThree clearfix">
      <%= render partial: 'common/main_people_screen', locals: { :is_editable => true, :parents => @parents_history, :godparents_list => @godparents } %>
    </div>
    <div class="tabLabelContainer managerTabFour clearfix">
      <%= render partial: 'manager/users_manager', locals: { :users => @users_list} %>
    </div>
  </div>
</div>
